<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginLeft="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/computer"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginEnd="47dp"
        android:layout_marginRight="47dp"
        android:background="@drawable/ece"
        android:layout_alignTop="@+id/imageButton10"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="@drawable/mechanical"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/textView13"
        android:layout_alignLeft="@+id/imageButton10"
        android:layout_alignStart="@+id/imageButton10" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="@drawable/civil"
        android:layout_alignTop="@+id/imageButton12"
        android:layout_alignLeft="@+id/imageButton11"
        android:layout_alignStart="@+id/imageButton11" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="@drawable/bba"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/textView19"
        android:layout_alignLeft="@+id/imageButton12"
        android:layout_alignStart="@+id/imageButton12" />

    <ImageButton
        android:id="@+id/imageButton15"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="@drawable/mba"
        android:layout_alignTop="@+id/imageButton14"
        android:layout_alignLeft="@+id/imageButton13"
        android:layout_alignStart="@+id/imageButton13" />

    <TextView
        android:textSize="19sp"
        android:fontFamily="cursive"
        android:textColor="#e9fc6712"
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Cse"
        android:layout_below="@+id/imageButton10"
        android:layout_alignRight="@+id/imageButton10"
        android:layout_alignEnd="@+id/imageButton10"
        android:layout_marginRight="36dp"
        android:layout_marginEnd="36dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="19sp"
        android:fontFamily="cursive"
        android:textColor="#e9fc6712"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Ece"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignRight="@+id/imageButton11"
        android:layout_alignEnd="@+id/imageButton11" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="19sp"
        android:fontFamily="cursive"
        android:textColor="#e9fc6712"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:layout_below="@+id/imageButton12"
        android:layout_marginTop="24dp"
        android:text="Mech" />

    <TextView
        android:id="@+id/textView20"
        android:textSize="19sp"
        android:fontFamily="cursive"
        android:textColor="#e9fc6712"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignEnd="@+id/textView16"
        android:layout_alignRight="@+id/textView16"
        android:text="Civil" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView19"
        android:layout_alignStart="@+id/textView19"
        android:layout_below="@+id/imageButton15"
        android:layout_marginTop="26dp"
        android:textSize="19sp"
        android:fontFamily="cursive"
        android:textColor="#e9fc6712"
        android:text="BBA" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView21"
        android:layout_alignBottom="@+id/textView21"
        android:layout_alignLeft="@+id/textView20"
        android:layout_alignStart="@+id/textView20"
        android:fontFamily="cursive"
        android:text="MBA"
        android:textColor="#e9fc6712"
        android:textSize="19sp" />
</RelativeLayout>
